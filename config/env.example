# Supabase Configuration
# Reemplaza estos valores con los de tu proyecto Supabase
SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Data Source Configuration
# true = usar datos reales de Supabase, false = usar mocks (solo desarrollo)
USE_SUPABASE=true

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Analytics (opcional)
NEXT_PUBLIC_GA_ID=

# WhatsApp Configuration
# Opción 1: URL completa de WhatsApp (prioridad más alta)
NEXT_PUBLIC_WA_URL=https://wa.me/569XXXXXXXX?text=Hola%20me%20interesa

# Opción 2: Número de teléfono E164 y mensaje (prioridad media)
WA_PHONE_E164=+569XXXXXXXX
WA_MESSAGE=Hola, me interesa la nueva experiencia de arriendo

# Feature Flags (Frontend Architecture V2)
NEXT_PUBLIC_FLAG_CARD_V2=0
NEXT_PUBLIC_FLAG_VIRTUAL_GRID=0
